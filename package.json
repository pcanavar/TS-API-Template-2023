{
  "name": "api-example2023",
  "version": "1.0.0",
  "main": "./src/index.js",
  "license": "MIT",
  "scripts": {
    "copytemplates": "cpy API/templates/**/* !**/*.ts dist/API/templates/ --cwd=src/ --parents",
    "build": "tsc && tsc-alias -p tsconfig.json && npm run copytemplates",
    "start": "node dist/index.js",
    "prod": "npm run build && npm run start",
    "dev": "ts-node-dev --clear --rs --respawn -r tsconfig-paths/register src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "apidoc": "^0.54.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/apidoc": "^0.50.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "cpy-cli": "^4.2.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}